package a;

import java.util.ArrayList;

public class Main {
	public static void main(String[] args) {
		
		ArrayList<Student> students = new ArrayList<>();
		
		String menu = "1. 학생추가 2. 모든 학생 정보 출력 3. 평균 점수 출력 4. 프로그램 종료";
		
		students.add(new Student("홍길동", 1L, 80));
		students.add(new Student("이순신", 2L, 90));
		students.add(new Student("짱구", 3L, 50));
		
		for (Student student: students) {
			student.printInfo();
		}
		
		
		
	}
}
